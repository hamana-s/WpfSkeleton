<Window x:Class="WpfSkeleton.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        mc:Ignorable="d"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding AppName.Value}" Height="450" Width="800">
    <Grid>
        <DockPanel Height="25">
            <Label>入力ファイル:</Label>
            <Button DockPanel.Dock="Right" Command="{Binding Execute}">実行</Button>
            <Rectangle Width="5" DockPanel.Dock="Right"/>
            <TextBox Text="{Binding InputPath.Value,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
        </DockPanel>
        
    </Grid>
</Window>
