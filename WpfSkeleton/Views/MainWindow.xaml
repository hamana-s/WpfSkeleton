<Window
    x:Class="WpfSkeleton.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="{Binding AppName.Value}"
    Width="800"
    Height="450"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <Grid>
        <DockPanel Height="25">
            <Label>入力ファイル:</Label>
            <Button Command="{Binding Execute}" DockPanel.Dock="Right">実行</Button>
            <Rectangle Width="5" DockPanel.Dock="Right" />
            <TextBox Text="{Binding InputPath.Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
        </DockPanel>
    </Grid>
</Window>